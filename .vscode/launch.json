    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Blazor WebAssembly App",
            "type": "blazorwasm",
            "request": "launch",
            "cwd": "${workspaceFolder}/CSEUtils.Interface",
            "debugServer": 5054,
            "url": "http://localhost:5045", 
            "browser": "chrome",
        },
        {
            "name": "Launch Electron App",
            "type": "node-terminal",
            "request": "launch",
            "cwd": "${workspaceFolder}/CSEUtils.Interface.Electron/bin/Release/net8.0/publish",
            "command": "npm start",
            "preLaunchTask": "Build"
        }

    ]
}